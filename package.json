{
  "name": "@trazable/changelog-generator",
  "version": "1.0.0",
  "description": "Custom trazable changelog generator based on conventional-commits",
  "main": "./src/index.js",
  "bin": "./bin/changelog-generator",
  "scripts": {
    "test": "./node_modules/.bin/eslint --max-warnings=0  .",
    "test:fix": "./node_modules/.bin/eslint --max-warnings=0  --fix .",
    "start": "node ."
  },
  "files": ["src", "bin"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trazable/changelog-generator.git"
  },
  "keywords": [
    "changelog",
    "trazable",
    "semver"
  ],
  "author": "Trazable",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Trazable/changelog-generator/issues"
  },
  "homepage": "https://github.com/Trazable/changelog-generator#readme",
  "dependencies": {
    "conventional-changelog-writer": "^4.0.17",
    "conventional-commits-parser": "^3.1.0",
    "git-raw-commits": "^2.0.7",
    "read-pkg": "^5.2.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@trazable/eslint-config": "^1.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@trazable/eslint-config"
    ]
  }
}
